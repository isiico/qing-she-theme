  "tokenColors": [
    {
      "name": "New keyword, logical operator keyword",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#ba8ef7"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#707a84"
      }
    },
    {
      "name": "Comment Shebang",
      "scope": [
        "comment.line.shebang"
      ],
      "settings": {
        "foreground": "#ff8ad1"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#939da5"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff6a80"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ba8ef7"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#939da5"
      }
    },
    {
